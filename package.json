{
  "name": "SYNTEST",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/core": "^1.54.1",
    "@sap/cds": "^5",
    "@sap/hana-client": "^2.12.25",
    "@sap/xsenv": "^3.1.1",
    "@sap/xssec": "^3.2.11",
    "express": "^4",
    "hdb": "^0.19.0",
    "passport": "^0.5.2"
  },
  "devDependencies": {
    "@sap/hdi-deploy": "4.3.3",
    "sqlite3": "^5.0.2"
  },
  "scripts": {
    "start": "cds run"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "hana"
      },
      "uaa": {
        "kind": "xsuaa"
      },
      "auth": {
        "[production]": {
          "strategy": "JWT"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  }
}